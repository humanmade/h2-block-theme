	"styles": {
		"blocks": {
			"core/heading": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--alt)"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/post-title": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--alt-700)"
							},
							"typography": {
								"textDecoration": "underline"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/search": {
				"css": "& .wp-block-search__inside-wrapper { border: none; padding: 0;}"
			}
		},
		"color": {
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--accent)",
					"text": "var(--wp--preset--color--neutral)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxxl)",
					"lineHeight": "1.3"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxl)",
					"lineHeight": "1.3"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xl)",
					"lineHeight": "1.3"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "1.3"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "1.56"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "1.3"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--alt-700)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--alt)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--default)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.56"
		}
