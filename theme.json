{
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#ff424a",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "#ff979b",
					"name": "Accent - 300",
					"slug": "accent-light"
				},
				{
					"color": "#dd2e36",
					"name": "Accent - 600",
					"slug": "accent-600"
				},
				{
					"color": "#4668df",
					"name": "Alt",
					"slug": "alt"
				},
				{
					"color": "#f7faff",
					"name": "Alt - 100",
					"slug": "alt-100"
				},
				{
					"color": "#d2e4ff",
					"name": "Alt - 300",
					"slug": "alt-300"
				},
				{
					"color": "#698bbd",
					"name": "Alt - 700",
					"slug": "alt-700"
				},
				{
					"color": "#345079",
					"name": "Alt - 900",
					"slug": "alt-900"
				},
				{
					"color": "#ffffff",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#131313",
					"name": "Contrast",
					"slug": "contrast"
				}
			]
		},
		"layout": {
			"contentSize": "620px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/heading": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--alt)"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						}
					}
				}
			},
			"core/post-title": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--alt-700)"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						}
					}
				}
			}
		},
		"color": {
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--accent)",
					"text": "var(--wp--preset--color--white)"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--alt-700)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--alt)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-font)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json"
}